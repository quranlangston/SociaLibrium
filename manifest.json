{
  "manifest_version": 3,
  "name": "Social Librium",
  "version": "1.0",
  "description": "Restore Balance to instagram Part 333",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://www.instagram.com/*"],
      "js": ["/popup/popup.js"]
    }
  ]
}

